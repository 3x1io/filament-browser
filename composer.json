{
    "name": "3x1io/filament-browser",
    "description": "File & Folders & Media Browser With Code Editor",
    "keywords": [
        "laravel",
        "code",
        "editor",
        "browser",
        "file",
        ".env editor",
        "ui",
        "filament"
    ],
    "homepage": "https://3x1.io",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "io3x1\\FilamentBrowser\\": "src/"
        }
    },
    "authors": [
        {
            "name": "3x1",
            "email": "info@3x1.io",
            "role": "CEO"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.0",
        "filament/filament": "^2.9",
        "spatie/laravel-package-tools": "^1.10"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "io3x1\\FilamentBrowser\\FilamentBrowserProvider"
            ]
        }
    },
    "prefer-stable": true
}
